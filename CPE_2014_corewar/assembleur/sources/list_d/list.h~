#ifndef LIST_H_
# define LIST_H_

#include <stdlib.h>

typedef struct		s_list
{
  char			*data;
  struct s_list		*next;
  struct s_list		*prev;
}			t_list;

int			my_put_in_list_first(t_list **list, char *data);
int			my_put_in_list_last(t_list **list, char *data);
int			my_show_list(t_list *list);

#endif
